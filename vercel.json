{
  "version": 2,
  "builds": [
    {
      "src": "api/*.js",
      "use": "@vercel/node"
    }
  ],
  "rewrites": [
    {
      "source": "/api/reviews",
      "destination": "/api/reviews.js"
    },
    {
      "source": "/api/adminLogin",
      "destination": "/api/adminLogin.js"
    }
  ]
}
